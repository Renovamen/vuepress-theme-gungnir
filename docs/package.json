    "@vuepress/client": "2.0.0-beta.45",
    "@vuepress/plugin-docsearch": "2.0.0-beta.45",
    "anywhere": "^1.6.0",
    "oh-vue-icons": "^1.0.0-rc3",
    "sass-loader": "^12.6.0",
    "vue": "^3.2.33",
    "vuepress": "2.0.0-beta.45",
    "vuepress-theme-gungnir": "workspace:*"
  }
}
