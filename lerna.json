{
  "npmClient": "yarn",
  "useWorkspaces": true,
  "version": "2.0.0-alpha.22",
  "command": {
    "version": {
      "allowBranch": [
        "main",
        "v2"
      ],
      "conventionalCommits": true,
      "exact": true,
      "ignoreChanges": [
        "**/*.md"
      ],
      "message": "build: publish %s"
    }
  }
}
